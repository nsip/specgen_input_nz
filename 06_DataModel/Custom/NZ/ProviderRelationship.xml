<DataObject name="ProviderRelationship"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"
			
            xmlns="http://sifassociation.org/SpecGen">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>ProviderRelationship</Element>
	<privacyRating>Low</privacyRating>
    <Description>Records a recognised relationship between two education Providers.</Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The SIF GUID for the relationship between two providers.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>    <!-- @RefId -->

  <Item>
    <Element>SourceProviderRefId</Element>
    <Characteristics>M</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The SIF GUID for the source provider in the relationship.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>    <!-- SourceProviderRefId -->
  <Item>
    <Element>SourceProviderLocalId</Element>
    <Characteristics>O</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The MoE organisation Id for the source provider in the relationship.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- SourceProviderLocalId -->
  <Item>
    <Element>RelatedProviderRefId</Element>
    <Characteristics>M</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The SIF GUID for the related provider in the relationship.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>    <!-- RelatedProviderRefId -->
  <Item>
    <Element>RelatedProviderLocalId</Element>
    <Characteristics>O</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The MoE organisation Id for the related provider in the relationship.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
</Item>    <!-- RelatedProviderLocalId -->
  <Item>
	  <Element>ExpirationDate</Element>
	  <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
	  <Description>
		  <p>The date after which the provider relationship is no longer active.</p>
	  </Description>
	  <Type name="xs:date"/>
  </Item>    <!-- ExpirationDate -->
  <Item>
      <Element>RelationshipRole</Element>
      <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
      <Description>
		  <p>Code denoting the role the Source provider plays in the relationship.</p>
      </Description>
      <Type ref="CodeSets" name="NZCodeSetsProviderRelationshipRoleType"/>
  </Item>    <!-- RelationshipRole -->
  <Item>
	  <Element>YearLevelList</Element>
	  <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
	  <Description>
		  <p>A list of YearLevels, denoting the year levels of the
		  learners being hosted or sent from the client or contributing provider</p>
	  </Description>
	  <Type ref="CommonTypes" name="YearLevelsType"/>
  </Item>    <!-- YearLevelList -->
</DataObject>
