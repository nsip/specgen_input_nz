<DataObject name="LearningSupportRegisterDirectory"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"

      xmlns="http://sifassociation.org/SpecGen">

  <Key>LearningSupportRegisterDirectoryRefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>LearningSupportRegisterDirectory</Element>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>Learning Support Needs are grouped by similarity. The Learning Support Directory maps
         those groups to Learning Support Interventions so when a Learning Support Need is 
         identified for the Student only applicable Interventions for the group it belongs 
         to are presented to the user. It also holds additional information about the Learning
          Support Interventions, e.g. Description, Funded By, Provided by.
      </p>
    </Description>
  </Item>
  <Item>
    <Element>LearningSupportRegisterDirectoryRefId</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
			<p>The universally unique identifier for the data object record that is used across systems.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>    <!-- LearningSupportRegisterDirectoryRefId -->
  <Item>
    <Element>LearningSupportRegisterDirectoryLocalId</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
			<p>The Id of the data object record that was provided by the system that the information 
			was originally sourced from.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- LearningSupportRegisterDirectoryLocalId -->

  <Item>
    <Element>AreaOfNeed</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>The area of need the learning support intervention is addressing.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsWellbeingCharacteristicCategoryType"/>
  </Item>    <!-- AreaOfNeed -->
  <Item>
    <Element>Need</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the need the learning support intervention is addressing.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsWellbeingCharacteristicSubCategoryType"/>
  </Item>    <!-- Need -->

  <Item>
    <Element>Intervention</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>The learning support intervention that addresses the area of need.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportInterventionType"/>
  </Item>    <!-- Intervention -->

  <Item>
    <Element>TargetAudience</Element>
    <Characteristics>M</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the target audience of the learning support intervention for the area of need.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportTargetAudienceType"/>
  </Item>    <!-- TargetAudience -->

  <Item>
    <Element>Resource</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the resource that is required to support the
      learning support intervention in addressing the area of
      need.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportResourceType"/>
  </Item>    <!-- Resource -->

  <Item>
    <Element>ResourcesOtherDesc</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Free text description describing the resources required for the intervention. Only 
        populated if a Resource of 'Other' is selected.</p>
    </Description>
    <Type name="xs:normalizedString"/>
  </Item>    <!-- ResourcesOtherDesc -->

  <Item>
    <Element>FundedBy</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting who can fund a learning support intervention.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportFundedByType"/>
  </Item>    <!-- FundedBy -->

  <Item>
    <Element>FundedByOtherDesc</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Free text description of who is funding the intervention. Only populated if a
         Funded By of 'Other' is selected.</p>
    </Description>
    <Type name="xs:normalizedString"/>
  </Item>    <!-- FundedByOtherDesc -->

  <Item>
    <Element>ProvidedBy</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting who can provide a learning support intervention.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportProvidedByType"/>
  </Item>    <!-- ProvidedBy -->

  <Item>
    <Element>ProvidedByOtherDesc</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Free text description of who is providing the intervention. Only populated if a
         Provided By of 'Other' is selected.</p>
    </Description>
    <Type name="xs:normalizedString"/>
  </Item>    <!-- ProvidedByOtherDesc -->

  <Item>
    <Element>DeliveredByDesc</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Free text description of who is delivered the intervention on behalf of the Provider.</p>
    </Description>
    <Type name="xs:normalizedString"/>
  </Item>    <!-- DeliveredByDesc -->

  <Item>
    <Element>PreRequisiteDesc</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Free text description of any pre-requisites required before the intervention can be implemented.</p>
    </Description>
    <Type name="xs:normalizedString"/>
  </Item>    <!-- PreRequisiteDesc -->
  
  <Item>.
    <Element>InterventionStatus</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the status of a learning support intervention, e.g. In Development, Active.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportInterventionStatusType"/>
  </Item>    <!-- InterventionStatus -->

</DataObject>
