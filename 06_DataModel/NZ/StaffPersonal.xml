<DataObject name="StaffPersonal"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"

	    xmlns="http://sifassociation.org/SpecGen">


  <Key>StaffPersonalRefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>StaffPersonal</Element>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>All the personal contact and demographic information relating to a staff member,
				who might be a teacher or other employee of a Provider.</p>
      <p>Non personal information relating to the staff member's relationship with Providers
        is stored in the StaffAssignment data object.</p>
    </Description>
  </Item>
  <Item>
    <Element>StaffPersonalRefId</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
			<p>The universally unique identifier for the data object record that is used across systems.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>    <!-- RefId -->
  <Item>
    <Element>StaffPersonalLocalId</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>Medium</privacyRating>
    <Description>
			<p>The Id of the data object record that was provided by the system that the information 
			was originally sourced from.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- LocalId -->

  <Item>
    <Element>ESLPairwiseId</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Medium</privacyRating>
    <Description>
      <p>The GUID used by Education Sector Logon (ESL) to identify the Staff Member</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- ESLPairwiseId -->

  <Item>
    <Element>SchoolSMSUserId</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Medium</privacyRating>
    <Description>
      <p>The identifier used by the school's SMS to identify the Staff Member</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- SchoolSMSUserId -->

  <Item>
    <Element>PersonInfo</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>High</privacyRating>
    <Description>
		<p>Personal Information </p>
    </Description>
    <Type ref="CommonTypes" name="PersonInfoType"></Type>
  </Item>    <!-- PersonInfo -->

  <Item>
    <Element>RecordAudit</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The audit details for a record, e.g. Created/Last Updated DateTime and Created/Last Updated By.</p>
    </Description>
    <Type ref="CommonTypes" name="RecordAuditType"></Type>
  </Item>    <!-- RecordAudit -->
  
  <xhtml:Example xmlns="" name="StaffPersonal">
    <StaffPersonal>
      <StaffPersonalRefId>d3e34f41-9d75-101a-8c3d-00aa001a1652</StaffPersonalRefId>
      <PersonInfo>
        <Name>
          <FamilyName>Smith</FamilyName>
          <GivenName>Fred</GivenName>
          <FullName>Fred Smith</FullName>
		      <Type>LGL</Type>
          <IsVerified>Y</IsVerified>
        </Name>
        <Address>
			    <Physical>
            <Type>TH</Type>
              <Line1>The Enchanted Wood</Line1>
              <Line2>533 Hogwart's Way</Line2>
            <Suburb>RD 1</Suburb>
            <City>Hogsmeade</City>
            <PostalCode>5780</PostalCode>
          </Physical>
        </Address>
        <Email>
          <Personal>
            <Address>fsmith@yahoo.com</Address>
          </Personal>
          <Work>
            <Address>freddy@gmail.com</Address>
          </Work>
        </Email>
        <Phone>
          <Home>
            <Number>0437-765-234</Number>
          </Home>
          <Work>
            <Number>03 9637-2289</Number>
            <Extension>72289</Extension>
          </Work>
        </Phone>
      </PersonInfo> 
      <RecordAudit>
        <CreatedDateTime>2019-01-01T03:15:00</CreatedDateTime>
        <CreatedBy>
          <RefId>44fb63b4-cfef-4820-8501-e7d1e54555cb</RefId>
          <Name>Bobby Test</Name>
        </CreatedBy>
      </RecordAudit>     
    </StaffPersonal>
  </xhtml:Example>
</DataObject>
