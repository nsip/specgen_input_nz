<DataObject name='StudentActivityParticipation'
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
      xmlns:json="http://json.org/"

      xmlns="http://sifassociation.org/SpecGen">

  <Key>RefId</Key>
  <EventsReported>true</EventsReported>
   <Item>
    <Element>StudentActivityParticipation</Element>
	  <privacyRating>Low</privacyRating>
    <Description>
      This object provides information on a co-curricular or extra-curricular activity in which a student participates during a given school year.
    </Description>
  </Item>    <!-- StudentActivityParticipation -->
  <Item>
    <Element>RefId</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
	  <Description>The ID (GUID) that uniquely identifies the instance of the object.</Description>
    <Type ref='CommonTypes' name='RefIdType'/>
  </Item>    <!-- RefId -->
  <Item>
    <Element>StudentPersonalRefId</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>GUID that identifies the student participating in the activity. </p>
    </Description>
    <Type ref='CommonTypes' name='IdRefType'/>
  </Item>    <!-- StudentPersonalRefId-->

  <if intl = 'nz'>
    <Item>
      <Element>StudentPersonalLocalId</Element>
      <Characteristics>O</Characteristics>
      <privacyRating>Low</privacyRating>
      <Description>
        <p>The local Id that identifies the student participating in the activity. </p>
      </Description>
      <Type ref='CommonTypes' name='LocalIdType'/>
    </Item>    <!-- StudentPersonalLocalId-->
    <Item>
      <Element>ProviderRefId</Element>
      <Characteristics>O</Characteristics>
	    <privacyRating>Low</privacyRating>
      <Description>
        <p>The Id (GUID) that uniquely identifies the Provider running the activity the student is participating in, e.g. schools.</p>
      </Description>
      <Type ref="CommonTypes" name="IdRefType"></Type>
    </Item>    <!-- ProviderRefId -->  
    <Item>
      <Element>ActivityGroup</Element>
      <Characteristics>O</Characteristics>
      <privacyRating>Low</privacyRating>
      <Description>
        <p>Standard codes denoting the group of the activity the student is participating in, e.g. sport, art, cultural, music.
        </p>
      </Description>
      <Type ref="CodeSets" name="NZCodeSetsStudentActivityGroupType"/>
    </Item>    <!-- ActivityGroup -->
    <Item>
      <Element>ActivityType</Element>
      <Characteristics>O</Characteristics>
      <privacyRating>Low</privacyRating>
      <Description>
        <p>Standard codes denoting the type of activity the student is participating in, e.g. rugby, drama, Kapa Haka, Violin.
        </p>
      </Description>
      <Type ref="CodeSets" name="NZCodeSetsStudentActivityTypeType"/>
    </Item>    <!-- ActivityType -->
    <Item>
      <Element>ActivityName</Element>
      <Characteristics>M</Characteristics>
	    <privacyRating>Low</privacyRating>
      <Description>
        <p>The name of the activity the student participated in.</p>
      </Description>
      <Type name='xs:normalizedString'/>
    </Item>    <!--ActivityName-->
    <Item>
      <Element>LearningArea</Element>
      <Characteristics>O</Characteristics>
      <privacyRating>Low</privacyRating>
      <Description>
        <p>The learning area the student activity the student is participating in relates to.</p>
      </Description>
      <Type ref="CodeSets" name="NZCodeSetsLearningArea"/>
    </Item>    <!-- LearningArea -->    
  </if>

  <if intl = 'au'>
    <Item>
      <Element>StudentActivityInfoRefId</Element>
      <Characteristics>M</Characteristics>
      <privacyRating>Low</privacyRating>
      <Description>
        <p>GUID that identifies the activity in which the student participates.</p>
      </Description>
      <Type ref='CommonTypes' name='IdRefType'/>
    </Item> <!-- StudentActivityInfoRef-->
    <Item>
      <Element>SchoolYear</Element>
      <Characteristics>O</Characteristics>
      <privacyRating>Low</privacyRating>
      <Description>
        <p>
          School year for which the information is applicable, expressed as the four-digit year in which the school year ends (e.g., "2009").
        </p>
      </Description>
      <Type ref="CommonTypes" name="SchoolYearType"></Type>
    </Item> <!--SchoolYear-->
  </if>
  <Item>
    <Element>ParticipationComment</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>Comment related to the student's participation in the activity. </p>
    </Description>
    <Type name='xs:normalizedString'/>
  </Item>    <!--ParticipationComment-->
  <Item>
    <Element>StartDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>This is the date from which the activity participation is valid (inclusive). </p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!--StartDate-->
  <Item>
    <Element>EndDate</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>This is the date through which the activity participation is valid (inclusive). If element is supported by the publisher/responder, should contain a date if the student withdraws from the activity before the end of the school year. </p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!--EndDate-->
  <Item>
    <Element>Role</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Specific role the student plays in the activity (e.g., band leader, student body president). </p>
    </Description>
    <Type name='xs:normalizedString'/>
  </Item>    <!-- Role -->
  <Item>
    <Element>RecognitionList</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>List of awards and other types of recognition given to the student for this activity. </p>
    </Description>
   <Type ref="CommonTypes" name="RecognitionListType"></Type>
  </Item>    <!--RecognitionList-->

    <xhtml:Example xmlns='' name="StudentActivityParticipation">
    <StudentActivityParticipation>
      <RefId>9ECC9683-0E02-406F-926C-1C4D3542D122</RefId>
      <StudentPersonalRefId>646C5D4A-C829-4886-A02B-971695C7BC06</StudentPersonalRefId>
      <ActivityGroup json:force-string="true">101</ActivityGroup>
      <ActivityType json:force-string="true">102</ActivityType>
      <ActivityName json:force-string="true">First XI</ActivityName>
      <ParticipationComment>First year of participation</ParticipationComment>
      <StartDate>2008-09-01</StartDate>
      <Role>Team Member</Role>
      <RecognitionList>
        <Recognition>Player of the Day</Recognition>
      </RecognitionList>
    </StudentActivityParticipation>

  </xhtml:Example>
</DataObject>
