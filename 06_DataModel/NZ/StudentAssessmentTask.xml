<DataObject name="StudentAssessmentTask"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"

      xmlns="http://sifassociation.org/SpecGen">
	
  <Key>StudentAssessmentTaskRefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>StudentAssessmentTask</Element>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>Record of a student's attempt at a particular assessment task.</p>
     </Description>
  </Item>
  <Item>
    <Element>StudentAssessmentTaskRefId</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>A GUID that identifies an instance of this object. </p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>    <!-- StudentAssessmentTaskRefId -->
  <Item>
    <Element>StudentAssessmentTaskLocalId</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Local identifier for the assessment task, if provided by the source system.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- StudentAssessmentTaskLocalId -->

  <Item>
    <Element>Student</Element>
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the student held in the StudentPersonal 
        data object who is being assessed against the assessment task.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- Student -->

  <Item>
    <Element>AssessmentTask</Element>    
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the AssessmentTask data object that the student assessment is for.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- AssessmentTask -->

  <Item>
    <Element>ProviderInvolvedList</Element>    
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>A list of the providers involved and their role, e.g.
         assessing provider, assessment provider, assessment location provider.</p>
    </Description>
    <Type ref="CommonTypes" name="ProviderInvolvedListType"></Type>
  </Item>    <!-- ProviderInvovledList -->

  <Item>
    <Element>ProviderCourse</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the ProviderCourse that the students are 
        participating in the assessment task for.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- ProviderCourse -->

  <Item>
    <Element>LearningStandard</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the LearningStandard that the student who is 
        participating in the assessment task is being assessed against.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- LearningStandard -->

  <Item>
    <Element>StaffInvolvedList</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>A list of the staff involved and their role in relation to the assessment task, 
        e.g. the staff members that supervise the undertaking of the assessment task by the student.</p>
    </Description>
    <Type ref="CommonTypes" name="StaffInvolvedListType"></Type>
  </Item>    <!-- StaffInvolvedList -->

  <Item>
    <Element>DateTimeAssessmentScheduled</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date and time the student is scheduled to be assessed against the assessment task.</p>
    </Description>
    <Type name="xs:dateTime"></Type>
  </Item>    <!-- DateTimeAssessmentScheduled -->

  <Item>
    <Element>DateTimeAssessmentOccurred</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date and time the student was assessed against the assessment task.</p>
    </Description>
    <Type name="xs:dateTime"></Type>
  </Item>    <!-- DateTimeAssessmentOccurred -->

  <Item>
    <Element>YearAssessmentOccurred</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The year the student was assessed against the assessment task.</p>
    </Description>
    <Type name="xs:gYear"></Type>
  </Item>    <!-- YearAssessmentOccurred -->

  <Item>
    <Element>AssessmentType</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the type of assessment the student is undergoing for the assessment task.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsAssessmentTypeType"/>
  </Item>    <!-- AssessmentType -->

  <Item>
    <Element>Status</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the current status of the student assessment task</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsAssessmentStatusType"/>
  </Item>    <!-- Status -->

  <Item>
    <Element>Timeframe</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the timeframe of the assessment.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsAssessmentTimeframeType"/>
  </Item>    <!-- AssessmentTimeframe -->

  <Item>
    <Element>NCEAStatusDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Date the current NCEA status was set.</p>
    </Description>
    <Type name="xs:date"/>
  </Item>    <!-- NCEAStatusDate -->

  <Item>
    <Element>AchievementAttempt</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The attempt number the student is making to pass this assessment, e.g. is it their first, second attempt.</p>
    </Description>
    <Type name="xs:int"></Type>
  </Item>    <!-- AchievementAttempt -->

  <Item>
    <Element>SubjectAreaList</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The list of Subject Area the assessment task pertains to.</p>
    </Description>
    <Type ref="CommonTypes" name="SubjectAreaListType"/>
  </Item>    <!-- SubjectAreaList -->  

  <Item>
    <Element>YearLevel</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Code denoting the Year Level of the Assessment Task the student is being assessed against.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsYearLevelCodeType"/>
  </Item>    <!-- YearLevel -->

  <Item>
    <Element>SpecialAssessmentConditionList</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The list of the special assessment conditions that the student is
          entitled to have when sitting this assessment task.</p>
    </Description>
    <Type ref="CommonTypes" name="SpecialAssessmentConditionListType"></Type>
  </Item>    <!-- SpecialAssessmentConditionList -->

  <xhtml:Example xmlns="" name="Student Assessment Task">
    <StudentAssessmentTask>
      <StudentAssessmentTaskRefId>359D7510-1AD0-A9D7-A8C3-DAD0A85103A2</StudentAssessmentTaskRefId>
      <Student>
        <RefId>cd106d6d-d9d5-41fb-9943-5feb967c9435</RefId>
        <LocalId>HP-850139</LocalId>
        <Name>Harry Potter</Name>
      </Student>
      <AssessmentTask>
        <RefId>D0A0A27A-D0A8-510A-D9D7-5101A8C3DA39</RefId>
      </AssessmentTask>
      <ProviderInvolvedList>
        <ProviderInvolved>
          <RefId>11737EA4-301C-ADCA-75C8-7214A7C46BDB</RefId>
          <ProviderRole>LOC</ProviderRole>
        </ProviderInvolved>
        <ProviderInvolved>
          <RefId>ad30d3a6-e663-4d5b-8975-6c4ffc745bfb</RefId>
          <ProviderRole>PRO</ProviderRole>
        </ProviderInvolved>
      </ProviderInvolvedList>
      <ProviderCourse>
        <RefId>2c55ec5e-7def-4815-ae08-caad3b9f274d</RefId>
      </ProviderCourse>
      <StaffInvolvedList>
        <StaffInvolved>
          <RefId>228a2cbe-93c4-46ba-91fa-b3ae689eb437</RefId>
          <Name>Prof. McGonnegal</Name>
          <StaffRole>INV</StaffRole>
        </StaffInvolved>
      </StaffInvolvedList>
      <DateTimeAssessmentScheduled>2018-11-25T09:00:00+12:00</DateTimeAssessmentScheduled>
      <AssessmentType>EXT</AssessmentType>
      <SubjectAreaList>
          <SubjectArea>PHYS</SubjectArea>
      </SubjectAreaList>
      <YearLevel>11</YearLevel>
    </StudentAssessmentTask>
  </xhtml:Example>
</DataObject>
