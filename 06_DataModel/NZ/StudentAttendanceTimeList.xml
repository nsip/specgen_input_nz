			<SchoolYear>2018</SchoolYear>
			<PeriodAttendanceList>
				<PeriodAttendance>
					<AttendanceCode>E</AttendanceCode>
					<AttendanceStatus>01</AttendanceStatus>
					<TimetableDayId json:force-string="true">4</TimetableDayId>
					<TimetablePeriodId>4A</TimetablePeriodId>
					<StartTime>09:00:00</StartTime>
					<EndTime>09:40:00</EndTime>
					<SubjectAreaList json:force-array="true">
						<SubjectArea json:force-string="true">3</SubjectArea>
					</SubjectAreaList>
					<TeacherList json:force-array="true">
						<StaffMember>
							<Name><FullName>Phineas McWhirter</FullName></Name>
							<StaffMemberRef>
								<LocalId>McGP</LocalId>
							</StaffMemberRef>
						</StaffMember>
					</TeacherList>
					<RoomList json:force-array="true">
						<RoomId>12P</RoomId>
					</RoomList>
					<AttendanceNote>Explained absence, but unjustified; period 4A</AttendanceNote>
				</PeriodAttendance>
				<PeriodAttendance>
					<AttendanceCode>E</AttendanceCode>
					<AttendanceStatus>01</AttendanceStatus>
					<TimetableDayId json:force-string="true">4</TimetableDayId>
					<TimetablePeriodId>4B</TimetablePeriodId>
					<StartTime>09:45:00</StartTime>
					<EndTime>10:30:00</EndTime>
					<SubjectAreaList json:force-array="true">
						<SubjectArea json:force-string="true">3</SubjectArea>
					</SubjectAreaList>
					<TeacherList json:force-array="true">
						<StaffMember>
							<Name><FullName>Phineas McWhirter</FullName></Name>
							<StaffMemberRef>
								<LocalId>McGP</LocalId>
							</StaffMemberRef>
						</StaffMember>
					</TeacherList>
					<RoomList json:force-array="true">
						<RoomId>12P</RoomId>
					</RoomList>
					<AttendanceNote>Explained absence, but unjustified; period 4B</AttendanceNote>
				</PeriodAttendance>
			</PeriodAttendanceList>
		</StudentAttendanceTimeList>
	</xhtml:Example>
</DataObject>
