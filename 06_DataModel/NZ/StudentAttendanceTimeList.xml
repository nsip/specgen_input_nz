<DataObject name="StudentAttendanceTimeList"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"
			
            xmlns="http://sifassociation.org/SpecGen">
	<Key>RefId</Key>

	<EventsReported>true</EventsReported>
	<Item>
		<Element>StudentAttendanceTimeList</Element>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>This object provides more detail about the attendance spans for a single student during the
				day: it provides more detailed information than the single TimeIn and TimeOut elements in
				StudentDailyAttendance.</p>
		</Description>
	</Item>
	<Item>
		<Element>RefId</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>The ID (GUID) that uniquely identifies the attendance record.</Description>
		<Type ref="CommonTypes" name="RefIdType"/>
	</Item>    <!-- RefId -->
	<Item>
		<Element>LocalId</Element>
		<Characteristics>O</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>	Local System Id for the Attendance Timelist record.</Description>
		<Type ref="CommonTypes" name="LocalIdType"/>
	</Item>    <!-- LocalId -->
	<Item>
		<Element>Student</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The ID (GUID) of the student for whom this attendance information is being reported.</p>
		</Description>
		<Type ref="CommonTypes" name="ObjectReference"/>
	</Item>    <!-- StudentPersonalRefId -->
	<Item>
		<Element>Provider</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The SIF GUID of the Provider who is making this attendance record.</p>
		</Description>
		<Type ref="CommonTypes" name="ObjectReference"/>
	</Item>    <!-- ProviderRefId -->
	<Item>
		<Element>AttendanceTakenTime</Element>
		<Characteristics>O</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The time the student's attendance was taken.</p>
		</Description>
		<Type name="xs:time"/>
	</Item>    <!-- AttendanceTakenTime -->
	<Item>
		<Element>Date</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The calendar date to which this attendance reporting period relates.</p>
		</Description>
		<Type name="xs:date"/>
	</Item>    <!-- Date -->
	<Item>
		<Element>SchoolYear</Element>
		<Characteristics>O</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The year for which the attendance record is being made.</p>
		</Description>
		<Type name="xs:gYear"/>
	</Item>    <!-- SchoolYear -->
	<Item>
		<Element>AttendanceTimeList</Element>
		<Characteristics>O</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>Breakdown of times that the student was recorded as being 
				present or absent from school or class.</p>
		</Description>
		<Type ref="CommonTypes" name="AttendanceTimeListType"/>
	</Item>    <!-- AttendanceTimes -->
	<Item>
		<Element>PeriodAttendanceList</Element>
		<Characteristics>O</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>List of any period attendances that need to be captured as part of a full list of 
			student attendance types.  Reserved for future use with TimeTableCell, Session or Scheduled Activity.
			</p>
		</Description>
		<Type ref="CommonTypes" name="PeriodAttendanceListType"/>
	</Item>    <!-- PeriodAttendances -->

  <xhtml:Example xmlns="" name="Student Attendance (By TimeList) Example">
		<StudentAttendanceTimeList>
			<RefId>45e9c39e-0e77-40b4-8ce7-6a6fa156249e</RefId>
			<Student>
				<RefId>7f11e3ed-3ac5-4673-9fdc-bcdbcd971811</RefId>
			</Student>
			<Provider>
				<RefId>e7c15fb4-48a6-4c11-b66f-fe93520444b6</RefId>
			</Provider>
			<Date>2018-06-26</Date>
			<SchoolYear>2018</SchoolYear>
			<AttendanceTimeList>
				<AttendanceTime json:force-array="true">
					<AttendanceCode>E</AttendanceCode>
					<AttendanceStatus>01</AttendanceStatus>
					<StartTime>09:00:00</StartTime>
					<EndTime>10:30:00</EndTime>
					<DurationValue>90</DurationValue>
					<AttendanceNote>Explained absence, but unjustified, 9:00 to 10:30am</AttendanceNote>
				</AttendanceTime>
			</AttendanceTimeList>
		</StudentAttendanceTimeList>
	</xhtml:Example>

