<DataObject name="StudentAttendanceTimeList"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"
			
            xmlns="http://sifassociation.org/SpecGen">
	<Key>StudentAttendanceTimeListRefId</Key>

	<EventsReported>true</EventsReported>
	<Item>
		<Element>StudentAttendanceTimeList</Element>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>This object provides more detail about the attendance spans for a single student during the
				day: it provides more detailed information than the single TimeIn and TimeOut elements in
				StudentDailyAttendance.</p>
		</Description>
	</Item>
	<Item>
		<Element>StudentAttendanceTimeListRefId</Element>
		<Characteristics>O</Characteristics>
		<privacyRating>Low</privacyRating>
			<p>The universally unique identifier for the data object record that is used across systems.</p>
		<Type ref="CommonTypes" name="RefIdType"/>
	</Item>    <!-- StudentAttendanceTimeListRefId -->
	<Item>
		<Element>Student</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The student for whom this attendance time list is being reported.</p>
		</Description>
		<Type ref="CommonTypes" name="ObjectReference"/>
	</Item>    <!-- StudentPersonalRefId -->
	<Item>
		<Element>Provider</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The provider who is providing this attendance time list record.</p>
		</Description>
		<Type ref="CommonTypes" name="ObjectReference"/>
	</Item>    <!-- ProviderRefId -->
	<Item>
		<Element>TeachingGroup</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>The teaching group that is reporting this student's attenandce time list.</p>
		</Description>
		<Type ref="CommonTypes" name="ObjectReference"/>
	</Item>    <!-- TeachingGroup -->
	<Item>
		<Element>AttendanceTimeList</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>Breakdown of times that the student was recorded as being 
				present or absent from school or class.</p>
		</Description>
		<Type ref="CommonTypes" name="AttendanceTimeListType"/>
	</Item>    <!-- AttendanceTimeList -->


	<xhtml:Example xmlns="" name="Student Attendance Time List">
		<StudentAttendanceTimeList>
			<Student>
				<RefId>7f11e3ed-3ac5-4673-9fdc-bcdbcd991234</RefId>
			</Student>
			<Provider>
				<RefId>7f11e3ed-3ac5-4673-9fdc-bcdbcd996698</RefId>
			</Provider>
			<TeachingGroup>
				<RefId>7f11e3ed-3ac5-4673-9fdc-bcdbcd993378</RefId>
			</TeachingGroup>
			<AttendanceTimeList>
				<AttendanceTime>
					<AttendanceTimeLocalId>7f11e3ed-3ac5-4673-9fdc-bcdbcd934554</AttendanceTimeLocalId>
					<AttendanceType>SCH</AttendanceType>
					<AttendanceCode>P</AttendanceCode>
					<AttendanceDate>2018-06-26</AttendanceDate>	
					<SchedulePeriod>
						<RefId>7f11e3ed-3ac5-4673-9fdc-bcdbcd999999</RefId>
					</SchedulePeriod>
					<SubmissionDate>2018-06-30</SubmissionDate>		
					<LastUpdatedDate>2018-06-28</LastUpdatedDate>
					<AttendanceNote>Student was present.</AttendanceNote>
				</AttendanceTime>	
				<AttendanceTime>
					<AttendanceTimeLocalId>7f11e3ed-3ac5-4673-9fdc-bcdbcd934667</AttendanceTimeLocalId>
					<AttendanceType>ADH</AttendanceType>
					<AttendanceCode>E</AttendanceCode>
					<AttendanceDate>2018-06-27</AttendanceDate>	
					<StartTime>09:45:00</StartTime>
					<EndTime>10:30:00</EndTime>
					<SubmissionDate>2018-06-30</SubmissionDate>		
					<LastUpdatedDate>2018-06-28</LastUpdatedDate>
					<AttendanceNote>Explained absence, but unjustified.</AttendanceNote>
				</AttendanceTime>
			</AttendanceTimeList>
		</StudentAttendanceTimeList>	
	</xhtml:Example>

</DataObject>
