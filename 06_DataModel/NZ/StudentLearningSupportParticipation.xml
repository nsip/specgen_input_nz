<DataObject name="StudentLearningSupportParticipation"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
      xmlns:json="http://json.org/"

      xmlns="http://sifassociation.org/SpecGen">
	
  <Key>RefId</Key>
  <EventsReported>true</EventsReported>
  
  <Item>
    <Element>StudentLearningSupportParticipation</Element>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>Contains information pertaining to a student's participation in learning support.</p>
     </Description>
  </Item>

  <Item>
    <Element>RefId</Element>
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The ID (GUID) assigned to uniquely identify this Student Learning Support Participation record.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>    <!-- RefId -->

  <Item>
    <Element>LocalId</Element>
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Locally assigned identifier for the learning support participation record, from the source system.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- LocalId -->

  <Item>
    <Element>Student</Element>    
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the student who participated in the learning support.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- Student -->

  <Item>
    <Element>AssignedTo</Element>    
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the staff member who has been assigned the student's learning support participation</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- AssignedTo -->

  <Item>
    <Element>CompletedDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the student's participation in the learning support intervention is considered complete.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- CompletedDate -->

  <Item>
    <Element>FundApprovedDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the learning support participation record for the student was created in the source system.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- FundApprovedDate -->

  <Item>
    <Element>RequestSentToAgencyDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the learning support participation request was sent to the agency providing the intervention.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- RequestSentToAgencyDate -->

  <Item>
    <Element>ParticipationStartDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the student started participating in the learning support.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- ParticipationStartDate -->

  <Item>
    <Element>ParticipationEndDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the student stopped participating in the learning support.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- ParticipationEndDate -->

  <Item>
    <Element>ParticipationNote</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Free text note pertaining to the student's learning support participation.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>    <!-- ParticipationNote -->

  <Item>
    <Element>PermissionEndDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the permission for learning support ended or was revoked, if provided, by the student's parent/caregiver.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- PermissionEndDate -->

  <Item>
    <Element>PermissionStartDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the permission for learning support was provided or was declined by the student's parent/caregiver.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- PermissionStartDate -->

  <Item>
    <Element>PermissionToParticipate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Indicates whether or not permission has been provided for the student to participate in the learning support.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsYesOrNoCategoryType"></Type>
  </Item>    <!-- PermissionToParticipate -->

  <Item>
    <Element>WellbeingCharacteristicList</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>A list of wellbeing characteristics that require learning support.</p>
    </Description>
    <Type ref="CommonTypes" name="WellbeingCharacteristicListType"></Type>
  </Item>    <!-- WellbeingCharacteristicList -->

  <Item>
    <Element>LearningSupportIntervention</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The learning support intervention the student is receiving.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportInterventionType"></Type>
  </Item>    <!-- LearningSupportIntervention -->

  <Item>
    <Element>InterventionOutcome</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The outcome of the student's participation on the learning support intervention. </p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportInterventionOutcomeType"></Type>
  </Item>    <!-- InterventionOutcomeType -->

  <Item>
    <Element>LearningSupportType</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The learning support type the student is participating in.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportTypeType"></Type>
  </Item>    <!-- LearningSupportType -->

  <Item>
    <Element>LearningSupportTypeOtherDesc</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Free text description of the learning support type that is being provided to the 
        student. Only populated if a Learning of Support Type of 'Other' is selected.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>    <!-- LearningSupportTypeOtherDesc -->

  <Item>
    <Element>ProvidedBy</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Code identifying the organisation that is providing the learning support.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportProvidedByType"></Type>
  </Item>    <!-- ProvidedBy -->

  <Item>
    <Element>ParticipationStatus</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The staus of the learning support participation.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsLearningSupportParticipationStatusType"></Type>
  </Item>    <!-- ParticipationStatus -->

  <Item>
    <Element>ReviewList</Element>    
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>A list of reviews made in regards to the student's learning support participation.</p>
    </Description>
    <Type ref="CommonTypes" name="LearningSupportReviewListType"></Type>
  </Item>    <!-- ReviewList -->

  <Item>
    <Element>CreatedBy</Element>    
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the staff member who created the student learning support participation record in the source system.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- CreatedBy -->

  <Item>
    <Element>LastUpdatedBy</Element>    
    <Characteristics>M</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>Generic object reference to the staff member who last updated the student's learning support participation record in the source system.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>    <!-- LastUpdatedBy -->

  <Item>
    <Element>CreatedDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the learning support participation record for the student was created in the source system.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- CreatedDate -->

  <Item>
    <Element>LastUpdatedDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The date the learning support participation record for the student was last updated in the source system.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- LastUpdatedDate -->

  <xhtml:Example xmlns="" name="StudentLearningSupportParticipation">
    <StudentLearningSupportParticipation>
      <RefId>2FFB63B4-CFEF-4820-8501-E7D1E54555CB</RefId>
      <Student>
        <RefId>CA285746-359D-7510-1A8C-36432A901A16</RefId>
      </Student>
    </StudentLearningSupportParticipation>
  </xhtml:Example>

</DataObject>
