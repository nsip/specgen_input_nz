<DataObject name="TeachingGroup"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://json.org/"

      xmlns="http://sifassociation.org/SpecGen">

  <Key>RefId</Key>

  <EventsReported>true</EventsReported>

  <Item>
    <Element>TeachingGroup</Element>
	  <privacyRating>Medium</privacyRating>
    <Description>
      <p>Identifies a particular class or other group of students.  Such groups 
        can have multiple staff assigned in a variety of roles, may be assigned students from multiple year levels
        and may be taught multiple courses.
      </p>
    </Description>
  </Item>

  <Item>
    <Element>RefId</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>The GUID of the TeachingGroup</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>       <!-- RefId -->

  <Item>
    <Element>LocalId</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>LocalId of the Teaching Group (previously ClassIndicator).</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>       <!-- LocalId -->

  <Item>
    <Element>ShortName</Element>
    <Characteristics>M</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Short free format label that describes the group.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>       <!-- ShortName -->

  <Item>
    <Element>LongName</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Longer description if required.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>       <!-- LongName -->

  <Item>
    <Element>GroupType</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>Provides the means of identifying the type of group such as roll class or subject class 
        for instance and treat them accordingly.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsTeachingGroupTypeType"/>
  </Item>       <!-- GroupType -->
  
   <Item>
    <Element>Provider</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>RefId  of the Provider who defines the TeachingGroup. Either RefId or LocalId must be provided.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>        <!-- ProviderRefId -->

  <Item>
    <Element>TeachingProvider</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>RefId of the Provider who actually teaches the TeachingGroup. Only present when the teaching provider is not the defining provider.</p>
    </Description>
    <Type ref="CommonTypes" name="ObjectReferenceType"></Type>
  </Item>        <!-- ProviderRefId -->

  <Item>
    <Element>SchoolYear</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>The SchoolYear that the class represented by the TeachingGroup is being taught.</p>
    </Description>
    <Type name="xs:gYear"></Type>
  </Item>       <!-- SchoolYear -->

  <Item>
    <Element>BeginDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The start date of the first Timetable that the class represented by the TeachingGroup
        is being taught in the nominated school year.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>       <!-- BeginDate -->

  <Item>
    <Element>EndDate</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>The end date of the last Timetable that the class represented by the TeachingGroup
        is being taught in the nominated school year.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>       <!-- EndDate -->

  <Item>
    <Element>TeachingSchedule</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Low</privacyRating>
    <Description>
      <p>List of schedule entries recording what ProviderCourses, Learning Areas or Subject Areas are being taught to the TeachingGroup when.</p>
    </Description>
    <Type ref="CommonTypes" name="TeachingGroupScheduleListType"></Type>
  </Item>        <!-- TeachingGroupScheduleList -->

  <Item>
    <Element>StudentList</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>Medium</privacyRating>
    <Description>
      <p>List of Students in this Teaching Group</p>
    </Description>
    <Type ref="CommonTypes" name="StudentListType"></Type>
  </Item>       <!-- StudentList -->

  <Item>
    <Element>TeacherList</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Medium</privacyRating>
    <Description>
      <p>A List of staff associated with the group â€“  may or may not include the teacher timetabled in the schedule.</p>
    </Description>
    <Type ref="CommonTypes" name="StaffListType"></Type>
  </Item>       <!-- TeacherList -->

  <Item>
    <Element>SubjectAreaList</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Medium</privacyRating>
    <Description>
      <p>List of codes denoting the Learning or Subject Areas of the Course the TeachingGroup is being taught.</p>
    </Description>
    <Type ref="CommonTypes" name="SubjectAreaListType"></Type>
  </Item>       <!-- SubjectAreaList -->

  <Item>
    <Element>YearLevelList</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>Medium</privacyRating>
    <Description>
      <p>For Subject Class teaching groups: the list of codes denoting the Year Levels
        of the Course the TeachingGroup is being taught.</p>
      <p>For Roll Class teaching groups, the Year Levels of the students
        in the roll class group.</p>
    </Description>
    <Type ref="CommonTypes" name="YearLevelListType"></Type>
  </Item>       <!-- YearLevelList -->

  <xhtml:Example xmlns="" name="TeachingGroup">
    <TeachingGroup>
      <RefId>64A309DA-063A-2E35-B359-D75101A8C3D1</RefId>
      <LocalId>20087ASPN</LocalId>
      <ShortName>7A SPN</ShortName>
      <LongName>Year 7A Maths - Space and Numbers</LongName>
      <GroupType json:force-string="true">2</GroupType>
      <Provider>
        <RefId>64A309DA-063A-2E35-B359-FACEA8C12312</RefId>
        <LocalId>234</LocalId> 
      </Provider>
      <SchoolYear>2018</SchoolYear>
      <BeginDate>2018-02-04</BeginDate>
      <EndDate>2018-12-05</EndDate>
      <StudentList>
        <Student>
          <StudentRef>
            <RefId>9897466F-200E-4BC1-B9AE-D1507DA15CEF</RefId>
            <LocalId>SMI001</LocalId>
          </StudentRef>
          <Name>
            <FamilyName>Smith</FamilyName>
            <GivenName>Peter</GivenName>
          </Name>
        </Student>
        <Student>
          <StudentRef>
            <RefId>7C834EA9-EDA1-2090-347F-83297E1C290D</RefId>
           <LocalId>SMI002</LocalId>
          </StudentRef>
          <Name>
            <FamilyName>Smith</FamilyName>
            <GivenName>Jennifer</GivenName>
          </Name>
        </Student>
        <Student>
          <StudentRef>
            <RefId>7C834EA9-EDA1-2090-347F-83297E1C290E</RefId>
           <LocalId>SMI003</LocalId>
          </StudentRef>
          <Name>
            <FamilyName>Smith</FamilyName>
