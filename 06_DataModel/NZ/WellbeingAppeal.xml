<DataObject name="WellbeingAppeal"
                          xmlns:xs="http://www.w3.org/2001/XMLSchema"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xmlns:xi="http://www.w3.org/2001/XInclude"
                        xmlns:xhtml="http://www.w3.org/1999/xhtml"
        
            xmlns="http://sifassociation.org/SpecGen">

  <Key>RefId</Key>
  
  <EventsReported>true</EventsReported>
  <Item>
    <Element>WellbeingAppeal</Element>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>Records detail of any "appeal" or "request" made by involved parties in regard to a negative/punitive Wellbeing Response that was the result of a student Wellbeing Event.</p>
    </Description>
  </Item>    <!-- WellbeingAppeal -->

  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
    <Description>
      <p>The ID (GUID) assigned to uniquely identify this WellbeingAppeal.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>    <!-- RefId -->

  <Item>
    <Element>StudentPersonalRefId</Element>
    <Characteristics>M</Characteristics>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>The ID (GUID) of the student to whom this WellbeingAppeal belongs.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>    <!-- StudentPersonalRefId -->

  <Item>
    <Element>ProviderRefId</Element>
    <Characteristics>M</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>The ID (GUID) of the school for which this WellbeingAppeal is being reported.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>    <!-- ProviderRefId -->

  <Item>
    <Element>WellbeingResponseRefId</Element>
    <Characteristics>M</Characteristics>
  	<privacyRating>High</privacyRating>
    <Description>
      <p>The ID (GUID) of the WellbeingResponse for which this WellbeingAppeal is in response to.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>    <!-- WellbeingResponseRefId -->

  <Item>
    <Element>WellbeingAlertRefId</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>The ID (GUID) of the Alert this WellbeingAppeal's outcome has triggered.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>    <!-- WellbeingAlertRefId -->

  <Item>
    <Element>WellbeingAppealLocalId</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>Local system Id of the Appeal.</p>
    </Description>
     <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item>    <!-- WellbeingAppealLocalId -->
  
  <Item>
    <Element>AppealStatus</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>High</privacyRating>
    <Description>
      <p>The status of WellbeingAppeal.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsWellbeingAppealStatusType"></Type>
  </Item>    <!-- AppealStatus -->
  
  <Item>
    <Element>RegisteredDate</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>Low</privacyRating>
    <Description>
      <p>The calendar date on which this WellbeingAppeal was registered.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>    <!-- RegisteredDate -->
    
  <Item>
    <Element>AppealNotes</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>Text relating to the particular WellbeingAppeal.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>    <!-- AppealNotes -->
  
  <Item>
    <Element>AppealOutcome</Element>
    <Characteristics>O</Characteristics>
  	<privacyRating>High</privacyRating>
    <Description>
      <p>The outcome of the appeal.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsWellbeingAppealOutcomeType"></Type>
  </Item>    <!-- AppealOutcome -->

  <Item>
    <Element>AppealDeterminationBy</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>The type of individual or group that determined the appeal outcome.</p>
    </Description>
    <Type ref="CodeSets" name="NZCodeSetsAppealDeterminationBy"></Type>
  </Item>    <!-- AppealDeterminationBy -->
 
  <Item>
    <Element>DocumentList</Element>
    <Characteristics>O</Characteristics>
	  <privacyRating>High</privacyRating>
    <Description>
      <p>Describes the Location of any associated documents kept in relation to this Appeal.</p>
    </Description>
     <Type ref="CommonTypes" name="DocumentListType"></Type>
  </Item>    <!-- DocumentList -->

  <xhtml:Example xmlns="" name="WellbeingAppeal">
    <WellbeingAppeal RefId="2FFB63B4-CFEF-4820-8501-E7D1E54555CB">
      <StudentPersonalRefId>D3E34B35-9D75-101A-8C3D-00AA001A1652</StudentPersonalRefId>
      <ProviderRefId>CA285746-359D-7510-1A8C-36432A901A16</ProviderRefId>
      <WellbeingResponseRefId>BC385746-359D-7510-1A8C-36432A901A36</WellbeingResponseRefId>
      <AppealStatus>SU</AppealStatus>
      <RegisteredDate>2017-11-01</RegisteredDate>
      <AppealNotes>This Appeal was successful.</AppealNotes>
    </WellbeingAppeal>

  </xhtml:Example>
</DataObject>
