	<CommonElement name="VerificationEventType" type="true" dm="true">
		<Item>
			<Element>VerificationEventType</Element>
			<Description>
				<p>A record of how information about a person has been
				verified. For example, verification of name, date of
				birth, citizenship.</p>
			</Description>
		</Item>
		<Item>
			<Element>IsConfirmed</Element>
			<Characteristics>M</Characteristics>
			<privacyRating>Low</privacyRating>
			<Description>
				<p>If true then verification has been confirmed by MoE.</p>
			</Description>
			<Type name="xs:boolean"/>
		</Item>    <!-- IsConfirmed -->
		<Item>
			<Element>Timestamp</Element>
			<Characteristics>O</Characteristics>
			<privacyRating>Low</privacyRating>
			<Description>
				<p>The date &amp; time that the VerificationEvent took place.</p>
			</Description>
			<Type name="xs:dateTime"/>
		</Item>    <!-- TimeStamp -->
		<Item>
			<Element>Method</Element>
			<Characteristics>M</Characteristics>
			<privacyRating>Low</privacyRating>
			<Description>
				<p>The means by which an identity and eligibility status
				for education services is verified.  Principally refers to
				an identity document type (eg passport) but may also
				include a type of business decision taken to accept an
				assertion of identity and eligibility status (eg principal
				discretion).</p>
			</Description>
			<Type ref="CodeSets" name="NZCodeSetsVerificationMethodType"/>
		</Item>    <!-- Method -->
		<Item>
			<Element>ProviderRefId</Element>
			<Characteristics>M</Characteristics>
			<privacyRating>Low</privacyRating>
			<Description>
				<p>The RefId of the provider (eg school) that
				provided the verification.</p>
			</Description>
			<Type ref="CommonTypes" name="IdRefType"/>
		</Item>    <!-- ProviderRefId -->
		<Item>
			<Element>ProviderLocalId</Element>
			<Characteristics>O</Characteristics>
			<privacyRating>Low</privacyRating>
			<Description>
				<p>The local identifier of the provider (eg school) that
				provided the verification.</p>
			</Description>
			<Type ref="CommonTypes" name="LocalIdType"/>
		</Item>    <!-- ProviderLocalId -->
		
