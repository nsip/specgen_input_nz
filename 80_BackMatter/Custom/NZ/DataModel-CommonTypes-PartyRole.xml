	<CommonElement name="CommunicationChannelType" type="true" dm="true">
		<Item>
			<Element>CommunicationChannelType</Element>
			<Description>
				<p>A particular person or organisation's communications method</p>
			</Description>
			<Type complex="extension" name="BaseCommunicationChannelType"/>
		</Item>	
		<Item>
			<Element>Type</Element>
			<Characteristics>O</Characteristics>
			<Description>
				<p>A code classifying the Communication's method</p>
			</Description>
			<Type ref="CodeSets" name="NZCodeSetsCommunicationChannelTypeType"/>
		</Item>    <!-- Type -->
		<Item>
			<Element>Value</Element>
			<Characteristics>M</Characteristics>
			<Description>
				<p>The unique value that provides access to the channel. eg: URL for a website;
				Facebook page addess, Twitter handle.</p>
			</Description>
			<Type name="xs:string"/>
		</Item>    <!-- Value -->
		<xhtml:Example xmlns="" name="Communication Channel Example">
			<CommunicationChannel>
				<Notes>School Attendance System (Self Service)</Notes>
				<Type json:force-string="true">1</Type>
				<Value>http://www.hogwarts.school.nz/Attendance</Value>
			</CommunicationChannel>
		</xhtml:Example>
	</CommonElement>

