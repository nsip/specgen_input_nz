<IncludeGroup
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	
	xmlns="http://sifassociation.org/SpecGen">

			<!-- IncludeGroup is syntactic sugar that makes this a well-formed XML file that can
				be validated separately; and allows the xi:include to have parse="xml"  SpecGen tool ignores it. -->


			<!-- Element types for the Wellbeing Domain -->

		<!-- Wellbeing Items-->   

		<CommonElement name="MedicationListType" type="true" dm="true">
			<Item>
				<Element>MedicationListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>A list of the medications required for this Wellbeing Characteristic.</p>
				</Description>
					<List mode="List"/>
			</Item>
			<Item>
					<Element>Medication</Element>
					<Characteristics>OR</Characteristics>
					<Description></Description>
				<Type ref="CommonTypes" name="MedicationType"/>
			</Item>
		</CommonElement>
		<CommonElement name="MedicationType" type="true" dm="true">
			<Item>
				<Element>MedicationType</Element>
				<Characteristics>OR</Characteristics>
				<Description>
					<p>The medication, dosage and frequency, required by the student to manage the Wellbeing Characteristic.</p>
				</Description>
			</Item> 
			<Item>
				<Element>MedicationName</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Name of medication required for the student.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>Dosage</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Dosage and strength of medication required for the student.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>Frequency</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Frequency of Administration.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>AdministrationInformation</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Information about how the medication is to be administered.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>Method</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Method of administration.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>MedicationConsentList</Element>
				<Characteristics>OR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="MedicationConsentListType"/>
			</Item>
		</CommonElement>

		<CommonElement name="MedicationConsentListType" type="true" dm="true">
			<Item>
				<Element>MedicationConsentListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>A list consents that have been provided for administration of a medicine to a student.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>MedicationConsent</Element>
				<Characteristics>OR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="MedicationConsentType"/>
			</Item>
		</CommonElement>
		<CommonElement name="MedicationConsentType" type="true" dm="true">
			<Item>
				<Element>MedicationConsentType</Element>
				<Characteristics>OR</Characteristics>
				<Description>
					<p>Contains information about when, by whom, and who consent was given to for the administration of a medicine to a student.</p>
				</Description>
			</Item> 
			<Item>
				<Element>StartDate</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The date from which the student or student caretaker consented to the medicine being administered to the student by a particular individual.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>EndDate</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The date the consent for administration of the medicine ends or is revoked.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>ConsentedBy</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>The student or student caregiver that has consented to the medicine being administered to the student by the school.</p>
				</Description>
				<Type ref="CommonTypes" name="ObjectReferenceType"/>
			</Item>
			<Item>
				<Element>ProviderApplicableTo</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>The provider the medication administartion consent is applicable to.</p>
				</Description>
				<Type ref="CommonTypes" name="ObjectReferenceType"/>
			</Item>
			<Item>
				<Element>AdministeredBy</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>The staff member at the school that have been given permission by the student or the student's caregiver to administer the medicine to the student.</p>
				</Description>
				<Type ref="CommonTypes" name="ObjectReferenceType"/>
			</Item>
		</CommonElement>
			
		<CommonElement name="WellbeingEventCategoryListType" type="true" dm="true">
			<Item>
				<Element>WellbeingEventCategoryListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>List of Categories of the WellbeingEvent.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>EventCategory</Element>
				<Characteristics>MR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="WellbeingEventCategoryType"></Type>
			</Item>
		</CommonElement>
		<CommonElement name="WellbeingEventCategoryType" type="true" dm="true">
			<Item>
				<Element>WellbeingEventCategoryType</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>The category of the Wellbeing Event.</p>
				</Description>        	 
			</Item>
			<Item> 
				<Element>Category</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>The category of the Wellbeing Event.</p>
				</Description>
				<Union>
					<Type ref="CodeSets" name="NZCodeSetsWellbeingEventCategoryType"/>
					<Type name="xs:string"/>
				</Union>	 
			</Item>	   <!-- EventCategory -->
			<Item>
				<Element>EventSubCategoryList</Element>
				<Characteristics>C</Characteristics>
				<Description>
					<p>List of Sub-Categories associated with the Wellbeing Event's Category.</p>
				</Description>
				<Type ref="CommonTypes" name="WellbeingEventSubCategoryListType"></Type>
			</Item>    <!-- EventSubCategoryList -->
		</CommonElement>
		<CommonElement name="WellbeingEventSubCategoryListType" type="true" dm="true">
			<Item>
				<Element>WellbeingEventSubCategoryListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>List of SubTypes associated with a Category.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>EventSubCategory</Element>
				<Characteristics>MR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="WellbeingEventSubCategoryType"/>
			</Item>    <!-- EventSubCategory -->
		</CommonElement>
		<CommonElement name="WellbeingEventSubCategoryType" type="true" dm="true">
			<Item>
				<Element>WellbeingEventSubCategoryType</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>The sub-category of the Wellbeing Event.</p>
				</Description>        	 
			</Item>
			<Item> 
				<Element>SubCategory</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>The sub-category of the Wellbeing Event.</p>
				</Description>
				<Union>
					<Type ref="CodeSets" name="NZCodeSetsWellbeingEventSubCategoryType"/>
					<Type name="xs:string"/>
				</Union>	 
			</Item>	   <!-- EventSubCategory -->
		</CommonElement>
		
		<CommonElement name="FollowUpActionListType" type="true" dm="true">
			<Item>
				<Element>FollowUpActionListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>List of Actions where an action resulted and is known – maybe just a link to a WellbeingResponse Object</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>FollowUpAction</Element>
				<Characteristics>OR</Characteristics>
				<Description>Container for the Follow Up details.</Description>
				<Type ref="CommonTypes" name="FollowUpActionType"/>
			</Item>    <!-- FollowUpAction -->
		</CommonElement>		
		<CommonElement name="FollowUpActionType" type="true" dm="true">
			<Item>
				<Element>FollowUpActionType</Element>
				<Characteristics>OR</Characteristics>
				<Description>
					<p>Repeatable Follow Up Action.</p>
				</Description>
			</Item> 	
			<Item>
				<Element>WellbeingResponseRefId</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The RefId of a WellbeingResponse Object if it exists.</p>
				</Description>
				<Type ref="CommonTypes" name="IdRefType"></Type>
			</Item>    <!-- WellbeingResponseRefId -->
			
				<Item>
						<Element>FollowUpDetails</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Optional free text to outline follow up.</p>
				</Description>
				<Type name="xs:string"></Type>  
				</Item>
				
				<Item>
						<Element>FollowUpActionCategory</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Category of Action - typing locally defined.</p>
				</Description>
				<Type name="xs:string"></Type>  
				</Item>
			
		</CommonElement>
		
		<CommonElement name="PersonInvolvementListType" type="true" dm="true">
			<Item>
				<Element>PersonInvolvementListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>List of People involved in the Wellbeing event/response.  Mandatory if more people than the student to which this event/response occurs is involved.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>PersonInvolvement</Element>
				<Characteristics>OR</Characteristics>
				<Description>A person (additional to the student) involved in a  Wellbeing event/response where required to be known.  </Description>
				<Type ref="CommonTypes" name="PersonInvolvementType"/>
			</Item>    <!-- PersonInvolvement -->
		</CommonElement>
		<CommonElement name="PersonInvolvementType" type="true" dm="true">
			<Item>
				<Element>PersonInvolvementType</Element>
				<Characteristics>OR</Characteristics>
				<Description>
					<p>A person (additional to the student) involved in a  Wellbeing event/response where required to be known.</p>
				</Description>
				<Type complex="extension" name="ObjectReferenceType"/>
			</Item> 
			<Item>
				<Element>HowInvolved</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Optional free text detailing involvement.</p>
				</Description>
				<Type name="xs:strnormalizedString"></Type>  
			</Item>
		</CommonElement>

		<CommonElement name="RecognitionListType" type="true" dm="true">
			<Item>
				<Element>RecognitionListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>List of awards and other types of recognition given to the student for this activity. </p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>Recognition</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>The nature of recognition given to the student for the successful completion of work in a co-curricular or extra-curricular activity.</p>
				</Description>
				<Type name="xs:normalizedString"/>
			</Item> <!--Recognition-->
		</CommonElement>
		
		<CommonElement name="WithdrawalTimeListType" type="true" dm="true">
			<Item>
				<Element>WithdrawalTimeListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Where a student is to be removed or withdrawn partially for particular times in a week within the Start and End dates of the Response.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>Withdrawal</Element>
				<Characteristics>MR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="WithdrawalType"/>
			</Item>
		</CommonElement>
		<CommonElement name="WithdrawalType" type="true" dm="true">
			<Item>
				<Element>WithdrawalType</Element>
				<Description>
					<p>Repeatable Withdrawal.</p>
				</Description>
			</Item>
			<Item>
				<Element>WithdrawalStartDate</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>The start date of the withdrawal.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>WithdrawalEndDate</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>The end date of the withdrawal.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>WithdrawalStartTime</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Start time of the Withdrawal on that day.</p>
				</Description>
				<Type name="xs:time"></Type>  
			</Item>
			<Item>
				<Element>WithdrawalEndTime</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>End time of the Withdrawal on that day.</p>
				</Description>
				<Type name="xs:time"></Type>  
			</Item>
			<Item>
				<Element>SchoolTerm</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The school term the withdrawal took place in.</p>
				</Description>
				<Type name="xs:integer"></Type>  
			</Item>
		</CommonElement>
		
		<CommonElement name="SuspensionContainerType" type="true" dm="true">
			<Item>
				<Element>SuspensionContainerType</Element>
				<Description>
					<p>Container for details of a Removal/Suspension.</p>
				</Description>
			</Item> 
			<Item>
				<Element>SuspensionCategory</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>Categorisation of a Suspension record to fall into, In School, Withdrawal, Expulsion/Exclusion etc.  In School is usually NOT formally recognised by a jurisdiction but managed at a school level where the student is temporarily withdrawn from normal classes. Expulsion/Exclusion may mean the student’s attendance at a school is terminated.
					</p>
				</Description>
				<Type ref="CodeSets" name="NZCodeSetsSuspensionCategoryType"/>
			</Item>
			<Item>
				<Element>WithdrawalTimeList</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p></p>
				</Description>
				<Type ref="CommonTypes" name="WithdrawalTimeListType"></Type>
			</Item>
			<Item>
				<Element>Duration</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The duration of a Suspension in days 0.00 – X.xx.  (Half Day = 0.5, Full Day = 1.0 or the percentage of any day.)  Must be provided for a Suspension when known.</p>
				</Description>
				<Type name="xs:decimal"></Type>
			</Item>
			<Item>
				<Element>AdvisementDate</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The date parent/carers were officially notified.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>ResolutionMeetingTime</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The date and time at which a meeting with the student’s parents/carers was held to resolve the Suspension.</p>
				</Description>
				<Type name="xs:dateTime"></Type>  
			</Item>
			<Item>
				<Element>ResolutionNotes</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Comments about how the Suspension has been resolved, including any reparative actions the student needs to take.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
					<Element>EarlyReturnDate</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The date at which the student was permitted to return to school earlier than the agreed duration would have allowed.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
		</CommonElement>

		<CommonElement name="DetentionContainerType" type="true" dm="true">
			<Item>
				<Element>DetentionContainerType</Element>
				<Description>
							<p>Container for details of a Detention.</p>
				</Description>
			</Item> 
			<Item>
				<Element>DetentionCategory</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>Categorisation of a Detention.</p>
				</Description>
				<Type ref="CodeSets" name="NZCodeSetsDetentionCategoryType"/>
			</Item>
			<Item>
				<Element>DetentionDate</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p></p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>DetentionLocation</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Eventually to be typed for initial draft will be string to try and determine a valid code set.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>DetentionNotes</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Any notes that can be provided regarding the need/response that led to the detention.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
		</CommonElement>

		<CommonElement name="PlanRequiredContainerType" type="true" dm="true">
			<Item>
				<Element>PlanRequiredContainerType</Element>
				<Description>
							<p>Container for Information outlining details of a possible Plan.  A Personalised Plan will be referenced by a separate Object.</p>
						</Description>
			</Item> 
			<Item>
				<Element>PlanRequiredList</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p></p>
				</Description>
				<Type ref="CommonTypes" name="PlanRequiredListType"></Type>
			</Item>
		</CommonElement>
		<CommonElement name="PlanRequiredListType" type="true" dm="true">
			<Item>
				<Element>PlanRequiredListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>List to contain information of any plans that may eventuate.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>Plan</Element>
				<Characteristics>OR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="WellbeingPlanType"/>
			</Item>
		</CommonElement>		
		<CommonElement name="WellbeingPlanType" type="true" dm="true">
			<Item>
				<Element>WellbeingPlanType</Element>
				<Characteristics>OR</Characteristics>
				<Description>
					<p>Repeatable Plan information.</p>
				</Description>
			</Item> 
			<Item>
				<Element>PersonalisedPlanRefId</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>The GUID of a PersonalisedPlan Object if it exists and it can be provided.</p>
				</Description>
				<Type ref="CommonTypes" name="IdRefType"></Type>  
			</Item>
			<Item>
				<Element>PlanNotes</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Any notes that can be provided regarding the need that led to the plan.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
		</CommonElement>
		
		<CommonElement name="AwardContainerType" type="true" dm="true">
			<Item>
				<Element>AwardContainerType</Element>
				<Description>
						<p>Container for details of an Award, where this response was in relation to a particular Award.</p>
				</Description>
			</Item> 
			<Item>
				<Element>AwardDate</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Date when to be awarded.</p>
				</Description>
				<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>AwardType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Type of Award.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>AwardDescription</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Description of the Award.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>AwardNotes</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Any comments/notes associated with the Award.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
		</CommonElement>

		<CommonElement name="OtherWellbeingResponseContainerType" type="true" dm="true">
			<Item>
				<Element>OtherWellbeingResponseContainerType</Element>
				<Description>
					<p>Container for details of any other response that can not be classified by previous types.</p>
				</Description>
			</Item> 
			<Item>
					<Element>OtherResponseDate</Element>
					<Characteristics>O</Characteristics>
					<Description>
						<p>Date of the other response</p>
					</Description>
					<Type name="xs:date"></Type>  
			</Item>
			<Item>
				<Element>OtherResponseDescription</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Description of the other response.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
			<Item>
				<Element>OtherResponseNotes</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Any comments/notes associated with the other response.</p>
				</Description>
				<Type name="xs:string"></Type>  
			</Item>
		</CommonElement>

		<CommonElement name="AcknowledgementListType" type="true" dm="true">
			<Item>
				<Element>AcknowledgementListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>A list of the providers and staff who have been sent the alerts and have acknowledged they have been read.</p>
				</Description>
				<List mode="List"/>
			</Item>
			<Item>
				<Element>Acknowledgement</Element>
				<Characteristics>MR</Characteristics>
				<Description></Description>
				<Type ref="CommonTypes" name="AcknowledgementType"/>
			</Item>
		</CommonElement>		
		<CommonElement name="AcknowledgementType" type="true" dm="true">
			<Item>
				<Element>AcknowledgementType</Element>
				<Characteristics>MR</Characteristics>
				<Description>
					<p>A provider or staff member that has been sent the alerts and have acknowledged they have been read.</p>
				</Description>
			</Item>
			<Item>
				<Element>Acknowledged</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>Indicates whether or not a staff member has viewed the alerts.</p>
				</Description>
				<Type ref="CodeSets" name="NZCodeSetsYesOrNoCategoryType"/>
			</Item>    <!-- Acknowledged -->
			<Item>
				<Element>AcknowledgedDate</Element>
				<Characteristics>M</Characteristics>
				<Description>
					<p>The date the provider acknowledged a staff member has viewed the alerts.</p>
				</Description>
				<Type name="xs:date"></Type>
			</Item>    <!-- AcknowledgedDate -->
			<Item>
				<Element>Provider</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>A provider who has been sent the alerts and have acknowledged they have been read.</p>
				</Description>
				<Type ref="CommonTypes" name="ObjectReferenceType"></Type>  
			</Item>    <!-- Provider -->
			<Item>
				<Element>StaffMember</Element>
				<Characteristics>O</Characteristics>
				<Description>
					<p>A staff member who has been sent the alerts and have acknowledged they have been read.</p>
				</Description>
				<Type ref="CommonTypes" name="ObjectReferenceType"></Type>  
			</Item>    <!-- StaffMember -->
		</CommonElement>

		<CommonElement name="KeyContactListType" type="true" dm="true">
			<Item>
				<Element>KeyContactListType</Element>
				<Characteristics>O</Characteristics>
				<Description>
